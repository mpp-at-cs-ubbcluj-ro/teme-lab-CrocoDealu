<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="412.0" prefWidth="289.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.LoginController">
   <children>
      <VBox alignment="CENTER" prefHeight="413.0" prefWidth="339.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name of app" textAlignment="CENTER" />
            <VBox prefHeight="176.0" prefWidth="319.0" spacing="10.0">
               <children>
                  <Label text="Username" />
                  <TextField fx:id="username" prefHeight="27.0" prefWidth="462.0" />
                  <Label fx:id="usernameErrorLabel" textFill="#e91515" />
                  <Label text="Password" />
                  <PasswordField fx:id="password" />
                  <Label fx:id="passwordErrorLabel" textFill="#e91515" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <Button fx:id="login" mnemonicParsing="false" onAction="#onLogin" prefHeight="27.0" prefWidth="64.5" text="Login" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
